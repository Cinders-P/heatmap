for item, itr in  #5e4fa2 #3288bd #66c2a5 #abdda4 #e6f598 #ffffbf #fee08b #fdae61 #f46d43 #d53e4f #9e0142
	.t{itr}
		fill: item
body
	background-color:  #c5e1a5
	display: flex
	align-items: center
	justify-content: center
	min-height: 100vh
	margin: 0
	font-family: sans-serif

.label
	font-size 14px

.title
	font-size: 2rem

#pic
	background-image: url('./bg.jpg')
	mix-blend-mode: multiply
	position: absolute
	min-height: 100%
	min-width: 100vw
	flex-grow: 1
	top: 0
	left: 0
	z-index: -1
	opacity: 0.5
	filter: brightness(0.3)
	box-shadow inset 0 0 50px 20px black


svg
	background-color: white
	box-shadow 0 0 50px rgba(0,0,0,0.2)

rect
	cursor pointer
	&:hover
		stroke: rgba(0,0,0,0.7)
		stroke-width 1px

rect[width='31']
	stroke: none
	cursor: auto

#box
	background-color: rgba(0,0,0,0.7)
	outline 2px black
	color: white
	font-size: 12px
	height: 54px
	padding: 13px 20px 13px 20px
	box-sizing: border-box
	position: absolute
